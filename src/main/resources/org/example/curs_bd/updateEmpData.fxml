                <Label layoutX="240.0" layoutY="14.0" text="Железка-Авто">
                    <font>
                        <Font size="30.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="13.0" layoutY="101.0" prefHeight="285.0" prefWidth="674.0" style="-fx-background-color: #f5f5dc;">
            <children>
                <Label fx:id="welcomeCl" layoutX="128.0" layoutY="38.0" text="Что вы хотите изменить?">
                    <font>
                        <Font size="38.0" />
                    </font>
                </Label>
